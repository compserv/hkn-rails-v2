<div>
<%= link_to 'Back', admin_cand_applications_path %>
</div>

<table id='applications'>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Email</th>
  </tr>
  <%- @candidates.each do |u| -%>
    <tr>
      <td><%= u.first_name %></td>
      <td><%= u.last_name  %></td>
      <td><%= u.phone_number %></td>
      <td><%= u.email %></td>
    </tr>
  <%- end -%>
</table>
