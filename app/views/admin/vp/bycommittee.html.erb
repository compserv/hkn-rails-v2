<div>
<%= link_to 'Back', admin_cand_applications_path %>
</div>

<div>
Second and third choice committees in parentheses.
</div>

<%- @mapping.each_pair do |comm, cands| -%>
  <div class="committee">
    <h1><%= comm ? comm : 'None' %></h1>
    <ul>
      <%- cands.each do |cand| -%>
        <li><%= cand.full_name %><%= cand.committee_preferences ? %{ (#{cand.committee_preferences.split[1]}, #{cand.committee_preferences.split[2]})} : ' None' %></li>
      <%- end -%>
    </ul>
  </div>
<%- end -%>