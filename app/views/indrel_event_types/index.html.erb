<h1>Listing Indrel Event Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @indrel_event_types.each do |indrel_event_type| %>
      <tr>
        <td><%= indrel_event_type.name %></td>
        <td><%= link_to 'Show', indrel_event_type %></td>
        <td><%= link_to 'Edit', edit_indrel_event_type_path(indrel_event_type) %></td>
        <td><%= link_to 'Destroy', indrel_event_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indrel event type', new_indrel_event_type_path %> | <%= link_to 'Indrel Admin Index', admin_indrel_path %>
